{
  "name": "tomcode",
  "displayName": "[tomcode]",
  "description": "tomcode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tomcode.select-window-theme",
        "title": "[tomcode] Select workspace theme"
      },
      {
        "command": "tomcode.edit-self",
        "title": "[tomcode] Edit tomcode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "mkdir -p dist && vsce package -o dist/tomcode.vsix --yarn",
    "deploy:local": "yarn package && code --install-extension dist/tomcode.vsix --force"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "^2.22.0"
  },
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "url": "https://github.com/tomjakubowski/tomcode"
  }
}
